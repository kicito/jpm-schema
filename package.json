{
  "name": "jpm-schema",
  "version": "1.0.0",
  "description": "This repository contains the description of npm's package.json file that is compatible with jolie package manager",
  "main": "dist/command.js",
  "scripts": {
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "build": "shx rm -rf dist && shx rm -f tsconfig.tsbuildinfo && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc.js --fix"
  },
  "maintainers": [
    {
      "name": "Narongrit Unwerawattana",
      "email": "narongrit.kie@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kicito/jpm-schema.git"
  },
  "keywords": [
    "jolie",
    "schema"
  ],
  "author": {
    "name": "Narongrit Unwerawattana",
    "email": "narongrit.kie@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kicito/jpm-schema/issues"
  },
  "homepage": "https://github.com/kicito/jpm-schema#readme",
  "devDependencies": {
    "@oclif/test": "^2",
    "@tsconfig/node16-strictest-esm": "^1.0.1",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "chai": "^4",
    "eslint": "^8.14.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "^10",
    "oclif": "^3",
    "shx": "^0.3.4",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.0",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1"
  },
  "oclif": {
    "bin": "jolie-schema",
    "dirname": "jolie-schema",
    "commands": "./dist/src/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "bin": {
    "jolie-schema": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "overrides": {
    "eslint-plugin-unicorn": "^42.0.0"
  }
}
